    @P(10)
    @Type.String(mime = GLSL_FRAGMENT_MIME)
    @OnChange(\"updateShader\")
    @Config.Port(false)
    String fragment;

    PShader shader;
    
    @P(1) double\[\] redCell;


    void updateUniforms() \{
        shader.set(\"time\", millis()/2);
        shader.set(\"resolution\", width, height);
        if(redCell != null) \{
            shader.set(\"redCell\", redCell\[0\], redCell\[1\]);
        \}
    \}

    @Override
    public void setup() \{
        updateShader();
        noStroke();
    \}

    @Override
    public void draw() \{
        if (shader == null) \{
            shader = createShader(DEFAULT_VERTEX_SHADER, fragment);
        \}
        shader(shader);
        updateUniforms();
