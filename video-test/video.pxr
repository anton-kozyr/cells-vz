@ /video root:video {
  #%autostart true
  #%praxis.version 4.1.1
  .renderer OpenGL
  .width 1920
  .height 1200
  .fps 60.0
  @ ./screen video:output {
    #%graph.x 862
    #%graph.y 161
    .always-on-top true
    .show-cursor true
  }
  @ ./stream video:capture {
    #%graph.x 660
    #%graph.y 111
    .device "udpsrc port=9999 ! application/x-rtp,encoding-name=JPEG,payload=26 ! rtpjpegdepay ! jpegdec"
    .resize-mode Scale
    .align-x 0.41333333333333333
    .align-y 0.4
    .zoom 0.5
    .source-width 1920
    .source-height 1080
  }
  @ ./start core:start-trigger {
    #%graph.x 456
    #%graph.y 111
  }
  ~ ./stream!out ./screen!in
  ~ ./start!out ./stream!play
}
