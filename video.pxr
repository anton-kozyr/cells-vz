  @ ./circles video:custom {
    #%graph.x 1248
    #%graph.y 367
    .code "

    @P(1) int count;
            
    LinkedList<Circle> l = new LinkedList<>();
    
    @Override
    public void setup() \{
        noStroke();
        for(int i = 0; i<count; ++i) \{
            l.add(new Circle());
        \}
    \}
    
    @In(1)
    public void addCircle(double ignored) \{
        l.add(new Circle());
        l.removeFirst();
    \}
    
    @Override
    public void draw() \{
        l.stream().forEach(Circle::draw);
    \}
    
    private class Circle \{
        private double x;
        private double y;
        private double d;
        private double r;
        private double g;
        private double b;
        
        public Circle() \{
            x = random(width);
            y = random(width);
            d = 50 + random(150);
            r = randomColor();
            g = randomColor();
            b = randomColor();
        \}
        
        private double randomColor() \{
            return 100 + random(155);
        \}
        
        public void draw() \{
            fill(r, g, b);
            ellipse(x, y, d, d);
        \}
    \}
"
    .count 10
  }
