    .code "

    @P(1) float\[\] fft;
    
    @Out(1) Output strobe;
    @Out(2) Output move;
    
//    LinkedList
    
    @Override
    public void setup() \{
        noStroke();
        fill(255);
    \}

    @Override
    public void draw() \{
//        log(WARNING, \"LEN: \" +fft.length);
        if (fft.length == 0) \{
            return;
        \}
        int bar = width/fft.length;
        
        int hiThreshold = fft.length/6;
        int medThreshold = (fft.length/8*7);
        double lows = 0, meds = 0, his = 0;
        for(int i = 0; i<fft.length; ++i) \{
            double lfft = max(0, log(fft\[i\]*700*i));
            if(i < hiThreshold) \{
                his += lfft;
            \} else if (i >= medThreshold) \{
                lows += lfft;
            \} else \{
                meds += lfft;
            \}
            rect(bar * i + bar/2, 0, bar-1, lfft*50);
        \}
//        log(WARNING, \"!\" + round(his) + \":\" + round(meds) + \":\" + round(lows) + \"| \" + hiThreshold + \":\" + medThreshold + \":\" + fft.length);
        his /= hiThreshold;
        meds /= medThreshold - hiThreshold;
        lows /= fft.length - hiThreshold - medThreshold;
//        log(WARNING, \":\" + round(his) + \":\" + round(meds) + \":\" + round(lows) + \"| \" + hiThreshold + \":\" + medThreshold + \":\" + fft.length);
        if (his > 8) \{
            move.send();
        \}
        if (meds > 6) \{
//            meds.send();
        \}
        if (lows > -10) \{ // WTF < 0 ?
            strobe.send();
        \}
    \}
"
